{
	"name": "game",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "APP_ENV=development S3_BUCKET_NAME=ch-default vite dev",
		"build": "SKIP_DB_INIT=true vite build",
		"preview": "APP_ENV=development S3_BUCKET_NAME=ch-default vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"test:bdd": "STEP_DELAY=0 cucumber-js",
		"test:bdd:record": "STEP_DELAY=1000 cucumber-js",
		"generate": "drizzle-kit generate:sqlite",
		"migrate": "tsx --tsconfig ./tsconfig.json ./src/migrate.ts",
		"dev:custom": "vite --config vite/config.dev.mjs",
		"build:custom": "vite build --config vite/config.prod.mjs",
		"push": "drizzle-kit push:sqlite"
	},
	"devDependencies": {
