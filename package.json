		"@sveltejs/adapter-node": "^2.1.2",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/forms": "^0.5.7",
		"@toast-ui/calendar": "^2.1.3",
		"@toast-ui/editor": "^3.2.2",
		"@types/better-sqlite3": "^7.6.9",
